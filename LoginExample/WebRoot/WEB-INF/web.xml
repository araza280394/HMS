<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LoginExample</display-name>
  <servlet>
   
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>ExamplePackage.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    
    <servlet-name>Register</servlet-name>
    <servlet-class>ExamplePackage.Register</servlet-class>
  </servlet>
  <servlet>
    
    <servlet-name>PatReg</servlet-name>
    <servlet-class>ExamplePackage.PatReg</servlet-class>
  </servlet>
  <servlet>

    <servlet-name>addPat</servlet-name>
    <servlet-class>ExamplePackage.addPat</servlet-class>
  </servlet>
  <servlet>
   
    <servlet-name>BaseDAO</servlet-name>
    <servlet-class>ExamplePackage.BaseDAO</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/servlet/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PatReg</servlet-name>
    <url-pattern>/servlet/PatReg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addPat</servlet-name>
    <url-pattern>/servlet/addPat</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BaseDAO</servlet-name>
    <url-pattern>/servlet/BaseDAO</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>LoginPage.jsp</welcome-file>
     <welcome-file>doctor.jsp</welcome-file>
     <welcome-file>LoginPage.jsp</welcome-file>
  
  </welcome-file-list>
</web-app>